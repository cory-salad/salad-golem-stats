# Plan Importer Environment Configuration

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=statsdb
POSTGRES_USER=devuser
POSTGRES_PASSWORD=devpass

# MixPanel (required)
MIXPANEL_API_KEY=your-mixpanel-api-key-here
MIXPANEL_JQL_URL=https://mixpanel.com/api/query/jql

# Data directory for JSON file staging
DATA_DIRECTORY=./data

# Minimum plan duration in milliseconds (default: 10 minutes)
MINIMUM_DURATION=600000

# Import interval in milliseconds (default: 6 hours)
IMPORT_INTERVAL=21600000

# Optional: Filter by organization names (comma-separated)
# Only data for these orgs will be fetched from MixPanel and imported
# Leave empty or unset to import all orgs
# ORG_NAME_FILTER=OrgName1,OrgName2,OrgName3
